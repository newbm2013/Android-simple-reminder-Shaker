<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alial_saeedi.atry">

    <application
        android:name="com.example.alial_saeedi.atry.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"

        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity"></activity>
        <activity
            android:name=".MainBoardUser"
            android:theme="@style/AppTheme.NoActionBar"></activity>

        <activity
            android:name=".PickDate"
            android:label="@string/app_name"
            android:parentActivityName="com.example.alial_saeedi.atry.Addevent" >

        </activity>
        <activity
            android:name=".DetailsActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.example.alial_saeedi.atry.Addevent" >

        </activity>


        <activity
            android:name=".Addevent"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
            <!--android:parentActivityName="com.example.ait.time_managementadmin.MainBoardUser" >-->

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.alial_saeedi.atry.MainBoardUser" />
        </activity>


        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>